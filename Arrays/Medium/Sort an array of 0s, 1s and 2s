class Solution {
    public void sortColors(int[] nums) {

        Map<Integer , Integer> countMap = new HashMap<>();

        for(int num : nums){
            countMap.merge(num , 1 , Integer::sum);
        }

        int idx=0;
        for(int col=0; col<=2 ; col++){
            int freq= countMap.getOrDefault(col , 0);
            for(int i= 0 ; i<freq ; i++){
                nums[idx]= col;
                idx++;
            }
        }
        
    }
}
