public static int mySqrt(int n){
        if(n < 2) return n;

        int low = 1;
        int high = n/2;
        int ans = 0;

        while(low <= high){
            long mid = low + (high - low) / 2;
            if(mid * mid <= n){
                ans = (int) mid;
                low = (int) mid + 1;
            } else{
                high = (int) mid - 1;
            }
        }
        return ans;
}
